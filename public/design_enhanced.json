{
  "project_name": "Script Oracle",
  "version": "2.0",
  "brand": {
    "name": "SCRIPT ORACLE",
    "tagline": "Multi-Dimensional Debugging",
    "description": "AI-powered insight meets gamified growth",
    "philosophy": "BREAKING PATCHING is a prayer, RUNNING RESOLVING is a triumph",
    "founder": "VALKARION (Original Name: Abhey Shahl)"
  },
  "themes": {
    "dark_mode": {
      "enabled": true,
      "backgrounds": {
        "primary": "#0A0A0A",
        "secondary": "#141414",
        "tertiary": "#1F1F1F",
        "card": "#262626",
        "surface": "#2D2D2D",
        "elevated": "#333333"
      },
      "colors": {
        "primary": {
          "neon_green": "#00FF88",
          "neon_green_hover": "#00E67A",
          "neon_green_light": "#33FF99",
          "electric_blue": "#00AAFF",
          "cyber_purple": "#8B5CF6"
        },
        "accent": {
          "gold": "#FFD700",
          "orange": "#FF6B35",
          "pink": "#FF69B4",
          "yellow": "#FFE600",
          "red": "#FF4444"
        },
        "text": {
          "primary": "#FFFFFF",
          "secondary": "#E0E0E0",
          "tertiary": "#B0B0B0",
          "muted": "#808080",
          "disabled": "#404040"
        },
        "status": {
          "success": "#00FF88",
          "error": "#FF4444",
          "warning": "#FFB800",
          "info": "#00AAFF",
          "operational": "#00FF88"
        },
        "borders": {
          "default": "#333333",
          "focus": "#00FF88",
          "hover": "#404040",
          "active": "#555555"
        }
      }
    },
    "light_mode": {
      "enabled": true,
      "backgrounds": {
        "primary": "#FFFFFF",
        "secondary": "#F8F9FA",
        "tertiary": "#F1F3F4",
        "card": "#FFFFFF",
        "surface": "#FAFBFC",
        "elevated": "#F5F7FA"
      },
      "colors": {
        "primary": {
          "neon_green": "#00CC6A",
          "neon_green_hover": "#00B85D",
          "neon_green_light": "#33D884",
          "electric_blue": "#0088CC",
          "cyber_purple": "#7C3AED"
        },
        "accent": {
          "gold": "#E6C200",
          "orange": "#E55A2B",
          "pink": "#E6559A",
          "yellow": "#E6CC00",
          "red": "#E63946"
        },
        "text": {
          "primary": "#1A1A1A",
          "secondary": "#2D2D2D",
          "tertiary": "#4A4A4A",
          "muted": "#6B7280",
          "disabled": "#9CA3AF"
        },
        "status": {
          "success": "#00CC6A",
          "error": "#E63946",
          "warning": "#F59E0B",
          "info": "#0088CC",
          "operational": "#00CC6A"
        },
        "borders": {
          "default": "#E5E7EB",
          "focus": "#00CC6A",
          "hover": "#D1D5DB",
          "active": "#9CA3AF"
        }
      }
    }
  },
  "typography": {
    "font_families": {
      "primary": "Inter, system-ui, -apple-system, sans-serif",
      "heading": "Space Grotesk, Inter, sans-serif",
      "display": "Orbitron, Space Grotesk, sans-serif",
      "monospace": "JetBrains Mono, Consolas, 'Courier New', monospace",
      "gaming": "Rajdhani, Space Grotesk, sans-serif"
    },
    "font_sizes": {
      "xs": "0.75rem",
      "sm": "0.875rem",
      "base": "1rem",
      "lg": "1.125rem",
      "xl": "1.25rem",
      "2xl": "1.5rem",
      "3xl": "1.875rem",
      "4xl": "2.25rem",
      "5xl": "3rem",
      "6xl": "3.75rem",
      "7xl": "4.5rem",
      "8xl": "6rem"
    },
    "font_weights": {
      "thin": 100,
      "light": 300,
      "normal": 400,
      "medium": 500,
      "semibold": 600,
      "bold": 700,
      "extrabold": 800,
      "black": 900
    },
    "line_heights": {
      "none": 1,
      "tight": 1.25,
      "snug": 1.375,
      "normal": 1.5,
      "relaxed": 1.625,
      "loose": 2
    },
    "letter_spacing": {
      "tighter": "-0.05em",
      "tight": "-0.025em",
      "normal": "0em",
      "wide": "0.025em",
      "wider": "0.05em",
      "widest": "0.1em"
    }
  },
  "spacing": {
    "scale": {
      "0": "0",
      "0.5": "0.125rem",
      "1": "0.25rem",
      "1.5": "0.375rem",
      "2": "0.5rem",
      "2.5": "0.625rem",
      "3": "0.75rem",
      "3.5": "0.875rem",
      "4": "1rem",
      "5": "1.25rem",
      "6": "1.5rem",
      "7": "1.75rem",
      "8": "2rem",
      "9": "2.25rem",
      "10": "2.5rem",
      "11": "2.75rem",
      "12": "3rem",
      "14": "3.5rem",
      "16": "4rem",
      "20": "5rem",
      "24": "6rem",
      "28": "7rem",
      "32": "8rem",
      "36": "9rem",
      "40": "10rem",
      "44": "11rem",
      "48": "12rem",
      "52": "13rem",
      "56": "14rem",
      "60": "15rem",
      "64": "16rem",
      "72": "18rem",
      "80": "20rem",
      "96": "24rem"
    },
    "container": {
      "max_width": "1400px",
      "padding": "1rem",
      "margin": "0 auto"
    }
  },
  "layout": {
    "breakpoints": {
      "xs": "475px",
      "sm": "640px",
      "md": "768px",
      "lg": "1024px",
      "xl": "1280px",
      "2xl": "1536px"
    },
    "grid": {
      "columns": 12,
      "gap": {
        "sm": "1rem",
        "md": "1.5rem",
        "lg": "2rem"
      }
    },
    "header": {
      "height": "72px",
      "padding": "0 2rem"
    },
    "sidebar": {
      "width": "320px",
      "collapsed_width": "80px",
      "padding": "1rem"
    },
    "content": {
      "max_width": "1200px",
      "padding": "2rem"
    },
    "footer": {
      "height": "auto",
      "min_height": "120px",
      "padding": "2rem"
    }
  },
  "components": {
    "buttons": {
      "primary": {
        "padding": "12px 24px",
        "border_radius": "8px",
        "font_weight": 600,
        "font_size": "0.875rem",
        "transition": "all 200ms ease",
        "dark": {
          "background": "#00FF88",
          "color": "#000000",
          "hover": {
            "background": "#00E67A",
            "transform": "translateY(-1px)",
            "box_shadow": "0 4px 12px rgba(0, 255, 136, 0.3)"
          }
        },
        "light": {
          "background": "#00CC6A",
          "color": "#FFFFFF",
          "hover": {
            "background": "#00B85D",
            "transform": "translateY(-1px)",
            "box_shadow": "0 4px 12px rgba(0, 204, 106, 0.3)"
          }
        }
      },
      "secondary": {
        "padding": "12px 24px",
        "border_radius": "8px",
        "font_weight": 500,
        "font_size": "0.875rem",
        "border": "1px solid",
        "background": "transparent",
        "transition": "all 200ms ease",
        "dark": {
          "color": "#FFFFFF",
          "border_color": "#333333",
          "hover": {
            "border_color": "#00FF88",
            "color": "#00FF88",
            "background": "rgba(0, 255, 136, 0.05)"
          }
        },
        "light": {
          "color": "#1A1A1A",
          "border_color": "#E5E7EB",
          "hover": {
            "border_color": "#00CC6A",
            "color": "#00CC6A",
            "background": "rgba(0, 204, 106, 0.05)"
          }
        }
      },
      "ghost": {
        "padding": "8px 16px",
        "border_radius": "6px",
        "font_weight": 500,
        "background": "transparent",
        "border": "none",
        "transition": "all 150ms ease",
        "dark": {
          "color": "#B0B0B0",
          "hover": {
            "color": "#FFFFFF",
            "background": "#262626"
          }
        },
        "light": {
          "color": "#6B7280",
          "hover": {
            "color": "#1A1A1A",
            "background": "#F1F3F4"
          }
        }
      }
    },
    "cards": {
      "default": {
        "border_radius": "16px",
        "padding": "24px",
        "border": "1px solid",
        "transition": "all 200ms ease",
        "dark": {
          "background": "#1F1F1F",
          "border_color": "#333333",
          "box_shadow": "0 4px 12px rgba(0, 0, 0, 0.3)"
        },
        "light": {
          "background": "#FFFFFF",
          "border_color": "#E5E7EB",
          "box_shadow": "0 4px 12px rgba(0, 0, 0, 0.05)"
        }
      },
      "elevated": {
        "border_radius": "20px",
        "padding": "32px",
        "border": "1px solid",
        "transition": "all 200ms ease",
        "dark": {
          "background": "#262626",
          "border_color": "#404040",
          "box_shadow": "0 8px 24px rgba(0, 0, 0, 0.4)"
        },
        "light": {
          "background": "#FFFFFF",
          "border_color": "#D1D5DB",
          "box_shadow": "0 8px 24px rgba(0, 0, 0, 0.08)"
        }
      },
      "gaming": {
        "border_radius": "12px",
        "padding": "20px",
        "border": "2px solid",
        "transition": "all 300ms ease",
        "dark": {
          "background": "linear-gradient(135deg, #1A1A1A 0%, #262626 100%)",
          "border_color": "#00FF88",
          "box_shadow": "0 0 20px rgba(0, 255, 136, 0.2)"
        },
        "light": {
          "background": "linear-gradient(135deg, #FFFFFF 0%, #F8F9FA 100%)",
          "border_color": "#00CC6A",
          "box_shadow": "0 0 20px rgba(0, 204, 106, 0.15)"
        }
      }
    },
    "navigation": {
      "header": {
        "height": "72px",
        "padding": "0 2rem",
        "border_bottom": "1px solid",
        "backdrop_filter": "blur(12px)",
        "dark": {
          "background": "rgba(20, 20, 20, 0.8)",
          "border_color": "#333333"
        },
        "light": {
          "background": "rgba(255, 255, 255, 0.8)",
          "border_color": "#E5E7EB"
        }
      },
      "sidebar": {
        "width": "320px",
        "padding": "1rem",
        "border_right": "1px solid",
        "dark": {
          "background": "#0A0A0A",
          "border_color": "#333333"
        },
        "light": {
          "background": "#FFFFFF",
          "border_color": "#E5E7EB"
        }
      },
      "menu_item": {
        "padding": "12px 16px",
        "border_radius": "8px",
        "margin": "2px 0",
        "transition": "all 150ms ease",
        "dark": {
          "color": "#B0B0B0",
          "hover": {
            "background": "#262626",
            "color": "#FFFFFF"
          },
          "active": {
            "background": "rgba(0, 255, 136, 0.1)",
            "color": "#00FF88",
            "border_left": "3px solid #00FF88"
          }
        },
        "light": {
          "color": "#6B7280",
          "hover": {
            "background": "#F1F3F4",
            "color": "#1A1A1A"
          },
          "active": {
            "background": "rgba(0, 204, 106, 0.1)",
            "color": "#00CC6A",
            "border_left": "3px solid #00CC6A"
          }
        }
      }
    },
    "forms": {
      "input": {
        "padding": "12px 16px",
        "border_radius": "8px",
        "border": "1px solid",
        "font_size": "0.875rem",
        "transition": "all 200ms ease",
        "dark": {
          "background": "#262626",
          "border_color": "#404040",
          "color": "#FFFFFF",
          "placeholder_color": "#808080",
          "focus": {
            "border_color": "#00FF88",
            "box_shadow": "0 0 0 2px rgba(0, 255, 136, 0.2)"
          }
        },
        "light": {
          "background": "#FFFFFF",
          "border_color": "#D1D5DB",
          "color": "#1A1A1A",
          "placeholder_color": "#9CA3AF",
          "focus": {
            "border_color": "#00CC6A",
            "box_shadow": "0 0 0 2px rgba(0, 204, 106, 0.2)"
          }
        }
      },
      "label": {
        "font_size": "0.875rem",
        "font_weight": 500,
        "margin_bottom": "6px",
        "dark": {
          "color": "#E0E0E0"
        },
        "light": {
          "color": "#2D2D2D"
        }
      }
    },
    "gaming_elements": {
      "xp_badge": {
        "padding": "4px 12px",
        "border_radius": "20px",
        "font_weight": 700,
        "font_size": "0.75rem",
        "background": "linear-gradient(45deg, #FFD700, #FFA500)",
        "color": "#000000",
        "box_shadow": "0 2px 8px rgba(255, 215, 0, 0.3)"
      },
      "streak_indicator": {
        "padding": "2px 8px",
        "border_radius": "6px",
        "font_weight": 600,
        "font_size": "0.75rem",
        "background": "#FF4500",
        "color": "#FFFFFF",
        "box_shadow": "0 2px 6px rgba(255, 69, 0, 0.3)"
      },
      "level_progress": {
        "height": "8px",
        "border_radius": "4px",
        "overflow": "hidden",
        "dark": {
          "background": "#333333",
          "fill_color": "#00FF88"
        },
        "light": {
          "background": "#E5E7EB",
          "fill_color": "#00CC6A"
        }
      },
      "badge_grid": {
        "display": "grid",
        "grid_template_columns": "repeat(auto-fill, minmax(100px, 1fr))",
        "gap": "16px",
        "badge_size": "80px"
      }
    },
    "dashboard": {
      "stats_card": {
        "padding": "20px",
        "border_radius": "12px",
        "border": "1px solid",
        "min_height": "120px",
        "dark": {
          "background": "#1F1F1F",
          "border_color": "#333333"
        },
        "light": {
          "background": "#FFFFFF",
          "border_color": "#E5E7EB"
        }
      },
      "heatmap": {
        "cell_size": "16px",
        "cell_gap": "2px",
        "border_radius": "2px",
        "colors": {
          "dark": {
            "empty": "#1A1A1A",
            "low": "#003d1a",
            "medium": "#00ff88",
            "high": "#FFD700"
          },
          "light": {
            "empty": "#F1F3F4",
            "low": "#c6f6d5",
            "medium": "#00CC6A",
            "high": "#E6C200"
          }
        }
      }
    },
    "modals": {
      "overlay": {
        "background": "rgba(0, 0, 0, 0.8)",
        "backdrop_filter": "blur(8px)"
      },
      "content": {
        "border_radius": "20px",
        "max_width": "600px",
        "padding": "32px",
        "border": "1px solid",
        "dark": {
          "background": "#1F1F1F",
          "border_color": "#333333"
        },
        "light": {
          "background": "#FFFFFF",
          "border_color": "#E5E7EB"
        }
      }
    }
  },
  "animations": {
    "transitions": {
      "fast": "100ms ease-out",
      "normal": "200ms ease-out",
      "slow": "300ms ease-out",
      "slower": "500ms ease-out"
    },
    "easing": {
      "ease_in": "cubic-bezier(0.4, 0, 1, 1)",
      "ease_out": "cubic-bezier(0, 0, 0.2, 1)",
      "ease_in_out": "cubic-bezier(0.4, 0, 0.2, 1)"
    },
    "hover_effects": {
      "lift": "translateY(-2px)",
      "scale": "scale(1.02)",
      "glow": {
        "dark": "0 0 20px rgba(0, 255, 136, 0.4)",
        "light": "0 0 20px rgba(0, 204, 106, 0.3)"
      }
    },
    "loading": {
      "spinner": "rotate 1s linear infinite",
      "pulse": "pulse 2s ease-in-out infinite",
      "bounce": "bounce 1s ease-in-out infinite"
    }
  },
  "effects": {
    "shadows": {
      "sm": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
      "base": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06)",
      "md": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
      "lg": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)",
      "xl": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04)",
      "2xl": "0 25px 50px -12px rgba(0, 0, 0, 0.25)",
      "neon": {
        "dark": "0 0 20px rgba(0, 255, 136, 0.3)",
        "light": "0 0 20px rgba(0, 204, 106, 0.2)"
      }
    },
    "gradients": {
      "gaming": {
        "dark": "linear-gradient(135deg, #1A1A1A 0%, #262626 100%)",
        "light": "linear-gradient(135deg, #FFFFFF 0%, #F8F9FA 100%)"
      },
      "neon": {
        "dark": "linear-gradient(45deg, #00FF88, #00AAFF)",
        "light": "linear-gradient(45deg, #00CC6A, #0088CC)"
      },
      "gold": "linear-gradient(45deg, #FFD700, #FFA500)"
    },
    "blur": {
      "sm": "blur(4px)",
      "base": "blur(8px)",
      "md": "blur(12px)",
      "lg": "blur(16px)",
      "xl": "blur(24px)"
    }
  },
  "specific_sections": {
    "landing": {
      "hero": {
        "min_height": "100vh",
        "padding": "4rem 2rem"
      },
      "features": {
        "grid_columns": 3,
        "gap": "2rem"
      }
    },
    "dashboard": {
      "grid_gap": "24px",
      "card_min_height": "200px"
    },
    "leaderboard": {
      "item_height": "60px",
      "rank_width": "60px",
      "avatar_size": "48px"
    },
    "pricing": {
      "card_width": "320px",
      "gap": "2rem"
    },
    "authentication": {
      "form_width": "400px",
      "social_button_height": "48px"
    }
  },
  "accessibility": {
    "focus_outline": "2px solid",
    "focus_offset": "2px",
    "min_contrast_ratio": 4.5,
    "touch_target_min": "44px",
    "motion_safe": {
      "reduce_motion": "prefers-reduced-motion: reduce"
    }
  },
  "patterns": {
    "cyberpunk": {
      "enabled": true,
      "neon_borders": true,
      "glitch_effects": false,
      "scan_lines": false
    },
    "gaming": {
      "xp_system": true,
      "badges": true,
      "leaderboards": true,
      "streaks": true
    }
  }
}